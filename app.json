{
  "expo": {
    "name": "Steppy",
    "slug": "camel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Steppy.png",
    "scheme": "com.steppy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "background-fetch",
          "background-processing"
        ]
      }
    },
    "android": {
      "minSdkVersion": 26,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.haseeb234.camel",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-health-connect",
      "expo-screen-orientation",
      [
        "expo-background-fetch",
        {
          "backgroundFetchTaskName": "background-step-task",
          "backgroundFetchMinimumInterval": 15
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d8a2072a-27f8-46b6-a61f-e535d75ee5c9"
      }
    },
    "owner": "haseeb240"
  }
}
